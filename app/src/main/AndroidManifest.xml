<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hywa.mddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".floatingactionbutton.FloatingActionButtonActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".cardview.CardViewActivity">
        </activity>
        <activity android:name=".translucent.TranclucentActivity">
        </activity>
        <activity
            android:name=".tab.TabActivity"
            android:label="@string/title_activity_tab" />
        <activity android:name=".palette.PaletteActivity" />
        <activity android:name=".toolbar.ToolbarActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>